<p id="notice"><%= notice %></p>
<h1><%= @task.name %></h1>
<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>Complete?</td>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @task.name %></td>
